# Environment configuration for Goal Intelligence Agent
# Database connection (episodic memory Postgres)
EPISODIC_DB_HOST=your_database_host
EPISODIC_DB_PORT=your_database_port
EPISODIC_DB_NAME=your_database_name
EPISODIC_DB_USER=your_database_user
EPISODIC_DB_PASSWORD=your_database_password

# OpenAI: set your API key here to change which key is used
OPENAI_API_KEY=your_openai_api_key
# Model to use (change if needed)
OPENAI_MODEL=gpt-4o-mini

# Logging
LOG_LEVEL=INFO

SQS_FINANCIAL_INSIGHT_QUEUE_URL=your_sqs_queue_url
SQS_ORCHESTRATOR_CALLBACK_QUEUE=your_orchestrator_callback_queue_url

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_SESSION_TOKEN=your_aws_session_token
AWS_REGION=us-east-1
